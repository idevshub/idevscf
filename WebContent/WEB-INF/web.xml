<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter-mapping>
    <filter-name>Custom</filter-name>
    <url-pattern>/eventrecordapi</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>Custom</filter-name>
    <filter-class>com.collegefest.filter.CustomFilter</filter-class>
  </filter>
  <servlet>
    <servlet-name>Eventcontroller</servlet-name>
    <servlet-class>com.collegefest.controller.EventDataAPI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Eventcontroller</servlet-name>
    <url-pattern>/eventrecordapi</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Usercontroller</servlet-name>
    <servlet-class>com.collegefest.controller.UserDataAPI</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Usercontroller</servlet-name>
    <url-pattern>/userdataapi</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EventPage</servlet-name>
    <jsp-file>/eventPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventPage</servlet-name>
    <url-pattern>/event/*</url-pattern>
  </servlet-mapping>
</web-app>